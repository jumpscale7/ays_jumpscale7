git.build.1                    =
    url:'http://git.aydo.com/aydo/qemu-ledis',

git.export.1                   =
    dest:'$(system.paths.base)/apps/kvm',
    link:'False',
    revision:,
    source:'root/apps/kvm',
    url:'http://git.aydo.com/binary/kvm',

param.base                     = '/opt/kvm'

ubuntu.packages                =
    'qemu-kvm',
    'qemu',
    'python-libvirt',
    'virt-viewer',
    'lrzip',
    'python-libvirt',
    'libvirt-bin',
    'bridge-utils',
    'curlftpfs',

