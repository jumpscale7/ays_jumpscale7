dependencies.1                 =
    name:'pylabs5_base',


env.process.1                  = 

#how many instances can be installed of this package, if 1 the instance name is always 'main'
instances.maxnr                = 1


#name of packages which need to be installed locally
platform.supported             =
    'linux64',


git.url = 'https://git.aydo.com/racktivity/ext-pylabs-core'

git.export.1                   =
    dest:'/opt/qbase5/',
    link:'False',
    nodirs:'False',
    source:'qbase5/*',
    merge:'True',
    url: $(git.url),

git.export.2                   =
    dest:'/opt/qbase5/apps/applicationserver/',
    link:'False',
    nodirs:'False',
    source:'apps/applicationserver/*',
    url: $(git.url),

git.export.3                   =
    dest:'/opt/qbase5/lib/pylabs/core/pylabs/',
    link:'False',
    nodirs:'False',
    source:'core/*',
    url: $(git.url),


git.export.4                   =
    dest:'/opt/qbase5/lib/pylabs/extensions/',
    link:'False',
    nodirs:'False',
    source:'extensions',
    url: $(git.url),

git.export.5                   =
    dest:'/opt/qbase5/lib/python/site-packages/',
    link:'False',
    nodirs:'False',
    source:'lib/*',
    url: $(git.url),

git.export.6                  =
    dest:'/opt/qbase5/utils/',
    link:'False',
    nodirs:'False',
    source:'utils/*',
    url: $(git.url),

git.export.7                   =
    dest:'/opt/qbase5/lib/pylabs/extensions/agent',
    link:'False',
    nodirs:'False',
    branch:'pylabs5',
    source:'agent/',
    url: 'https://git.aydo.com/racktivity/ext-pylabs_agent',

git.export.8                   =
    dest:'/opt/qbase5/apps/applicationserver/services/agent_service',
    link:'False',
    branch:'pylabs5',
    nodirs:'False',
    source:'agent_service/',
    url: 'https://git.aydo.com/racktivity/ext-pylabs_agent',

git.export.9                   =
    dest:'/opt/qbase5/www/jswizards',
    link:'False',
    nodirs:'False',
    branch:'coffee',
    source:'',
    url: 'https://git.aydo.com/racktivity/ext-jswizards',

git.export.10                   =
    dest:'/opt/qbase5/www/jswizards/js/',
    link:'False',
    nodirs:'False',
    branch:'master',
    source:'js/jswizards.js',
    url: 'https://git.aydo.com/binary/jswizards',



git.export.15                   =
    dest:'/opt/qbase5/lib/python/site-packages/osis',
    branch:'0.5',
    link:'False',
    nodirs:'False',
    source:'code/osis',
    url: 'https://git.aydo.com/racktivity/ext-OSIS',

git.export.16                   =
    dest:'/opt/qbase5/lib/pylabs/extensions/api_generator/defaults',
    link:'False',
    nodirs:'False',
    source:'pyapp',
    url: 'https://git.aydo.com/racktivity/ext-OSIS',

git.export.17                   =
    dest:'/opt/qbase5/lib/pylabs/extensions/osis_connection',
    link:'False',
    branch:'0.5',
    nodirs:'False',
    source:'osis_connection_config',
    url: 'https://git.aydo.com/racktivity/ext-OSIS',

git.export.18                   =
    dest:'/opt/qbase5/www/lfw',
    link:'False',
    nodirs:'False',
    source:'htdocs',
    branch:'js7',
    url: 'https://git.aydo.com/racktivity/ext-lfw',

git.export.19                   =
