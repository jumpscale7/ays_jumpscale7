dependencies.1                 =
    name:'go',
    type:'build',

git.build.1                    =
    url:'https://github.com/Jumpscale/jsagentcontroller.git',

#known var's are: $root,$base,$cfg,$jslib,$pylib,$lib
git.export.1                   =
    dest:'$(system.paths.base)/apps/jsagentcontroller_go',
    link:'True',
    source:'.',
    url:'https://git.aydo.com/binary/jsagentcontroller_go/',

#base of repo where code or files are e.g. http://git.aydo.com/binary/base
git.url                        = 'https://git.aydo.com/binary/jsagentcontroller_go/'

#how many instances can be installed of this package, if 1 the instance name is always 'main'
instances.maxnr                = 10

platform.supported             =
    'linux64',

process.1                      =
    args:'-c agentcontroller.toml',
    cmd:'./jsagentcontroller',
    cwd:'$(system.paths.base)/apps/jsagentcontroller_go/',
    env:,
    filterstr:'jsagentcontroller -c',
    ports:'$(instance.param.webservice.host)',
    prio:'20',
    startupmanager:'tmux',
    timeout_start:'20',
    timeout_stop:'20',

