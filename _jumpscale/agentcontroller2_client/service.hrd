git.build.1                    =
    url:'https://github.com/Jumpscale/agentcontroller2.git',

#known var's are: $root,$base,$cfg,$jslib,$pylib,$lib
git.export.1                   =
    dest:'$(system.paths.python.lib.ext)/acclient.py',
    link:'True',
    source:'acclient.py',
    url:'https://git.aydo.com/binary/agentcontroller2_client/',

git.export.2                   =
    dest:'$(system.paths.python.lib.js)/libext/acclient_ext',
    link:'True',
    source:'acclient_ext/*',
    url:'https://git.aydo.com/binary/agentcontroller2_client/',
