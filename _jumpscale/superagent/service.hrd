dependencies.1                 =
    name:'go',
    type:'build',

git.build.1                    =
    url:'https://github.com/Jumpscale/jsagent.git',

git.export.1                   =
    dest:'$(system.paths.base)/apps/superagent/superagent',
    link:'True',
    source:'superagent',
    url:'https://git.aydo.com/binary/superagent',

git.export.2                   =
    dest:'$(system.paths.base)/apps/superagent/superagent.toml',
    link:'True',
    source:'agent.toml',
    url:'https://git.aydo.com/binary/superagent',

git.export.3                   =
    dest:'$(system.paths.base)/apps/superagent/extensions',
    link:'True',
    source:'extensions',
    url:'https://git.aydo.com/binary/superagent',

git.url                        = 'https://git.aydo.com/binary/superagent'

instances.maxnr                = 1

platform.supported             =
    'linux64',

process.1                      =
    args:,
    cmd:'./superagent -c $(system.paths.base)/apps/superagent/superagent.toml',
    cwd:'$(system.paths.base)/apps/superagent/',
    env:,
    filterstr:'superagent -c',
    ports:,
    prio:'5',
    startupmanager:'tmux',
    timeout_start:'60',
    timeout_stop:'10',

